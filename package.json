{
  "name": "torre-electron-react",
  "version": "1.0.0",
  "description": "a template for electron with react",
  "main": "src/main/main.js",
  "author": "wagnxx <wagnxx@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:mac": "cross-env BUILD_TARGET=MAC node script/build.js",
    "build:linux": "cross-env BUILD_TARGET=LINUX node script/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node script/build.js",
    "dev": "cross-env NODE_ENV=development node ./script/dev.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config config/webpack.config.js",
    "lint": "eslint --ext .js,.jsx -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.jsx -f ./node_modules/eslint-friendly-formatter --fix src"
  },
  "dependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5"
  },
  "devDependencies": {
    "electron": "^4.1.1",
    "del": "^4.0.0",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "sass-loader": "^7.1.0",
    "node-sass": "^4.11.0",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.10",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cfonts": "^2.4.2",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.12.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-typescript": "^0.14.0",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^3.13.0",
    "multispinner": "^0.2.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.1",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  }
}
